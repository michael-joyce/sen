<table class="table table-bordered table-condensed table-hover table-striped">
    <thead>
        <tr>
            <th>ID</th>
            <th>Name</th>

        </tr>
    </thead>
    <tbody>
        {% for notary in notaries %}
            <tr>
                <td>
                    <a href="{{ path('notary_show', { 'id': notary.id }) }}">
                        {{ notary.id }}
                    </a>
                </td>
                <td>
                    {{ notary.name }}
                </td>

            </tr>
        {% endfor %}
    </tbody>
</table>
